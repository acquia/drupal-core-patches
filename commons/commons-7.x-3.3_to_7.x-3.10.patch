+
+  ul.quicktabs-tabs {
+    margin: 0;
+    padding: 0;
+    position: relative;
+    top: 1px;
+
+    .block-content & {
+      padding: 0;
+    }
+
+    li {
+      display: inline-block;
+      padding: 0;
+      margin: 0 5px 0 0;
+
+      a {
+        background-color: $gray_darkest;
+        border: 1px solid $gray_darkest;
+        @include border-radius(3px 3px 0 0);
+        padding: 10px;
+        display: block;
+        font-size: 1.1em;
+        color: $link_color;
