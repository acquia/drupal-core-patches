+
+  li {
+    &:hover,
+    &.sfHover {
+      outline: 0;
+    }
+  }
+
+  a {
+    &:focus,
+    &:hover,
+    &:active {
+      outline: 0;
+    }
+  }
+}
+
+/**
+ * Superfish blocks
+ */
+
+.block-superfish {
+  ul {
+    margin: 0 !important;
+    padding: 0 !important;
+  }
+
+  li {
+    margin: 0 !important;
+    padding: 0 !important;
+  }
