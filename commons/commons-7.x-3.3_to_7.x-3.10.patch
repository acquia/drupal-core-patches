+  foreach($facets as $facet) {
+    $facet['settings'] = serialize($facet['settings']);
+    $facet['name'] = $env_id . $facet['name'];
+    $facet['searcher'] = $env_id;
+    $merge = db_merge('facetapi')
+      ->key(array('name' => $facet['name']))
+      ->fields($facet)
+      ->execute();
+  }
