+ * Implements hook_field_formatter_info().
+ */
+function commons_groups_field_formatter_info() {
+  return array(
+    'commons_groups_group_subscribe' => array(
+      'label' => t('Commons groups subscribe link'),
+      'field types' => array('list_boolean'),
