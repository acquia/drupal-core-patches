+    'page callback' => 'commons_trusted_contacts_ignore_trust',
+    'page arguments' => array(1),
+    'access callback' => 'commons_trusted_contacts_ignore_trust_access',
+    'access arguments' => array(1, 2),
+    'type' => MENU_CALLBACK,
