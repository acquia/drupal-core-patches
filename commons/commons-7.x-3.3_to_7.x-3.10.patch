+
+/**
+ * Button styles
+ */
+
+// Generate the invisible classes that we will extend.
+@include buttons;
+
+.button,
+button,
+[type="reset"],
+[type="submit"],
+[type="button"],
+[class*="action-item"] {
+  @extend %button-default-layout;
+  @extend %button-default;
+  max-width: 100%;
+
+  &:not(.action-item-inline, .action-item-merge) {
+    @include respond(only-small) {
+      display: block;
+      width: 100%;
+      margin: .5em 0;
+    }
